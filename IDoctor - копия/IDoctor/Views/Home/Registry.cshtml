@{
    ViewBag.Title = "Registry";
}
<header>
       <link rel="stylesheet" type="text/css" href="~/Content/MyStyles.css"/>
       <script>
           function makevisible() {
               $('#hiddenText').show();

           }
           function makeinvisible() {
               $('#hiddenText').hide();
           }
           function reload(){
               window.location.reload();
           }
       </script>
</header>
<body>
  <div id="head" style="width:100%">
    <img id="shapka" src="~/Images/Reg.png" style="left:0px;top:0px; position:absolute;z-index:5"/>
    <img id="shapka2" src="~/Images/bg3.png" style="left:0px;top:0px; position:absolute; z-index:1; width:100%; height:133px"/>
  </div>
  
    <form id="content" action="/Home/GetIndex" method="post">
        <div>
         <label id="addpat">Add patient</label>
         <div id="hiddenText" hidden="hidden">
         <label hidden="hidden" id="reg_part">@Html.Partial("GetIndex")</label>
        </div>
            <br />
         <hr id="sep" />  
        </div>
         <table>
            <tr>
                <td id="cell1"><label id="l">Last Name</label></td>
                <td><input id="T" name="txt" type="text" /></td>

            </tr>
            <tr>
                <td id="cell2"><label id="l2">First Name</label></td>
                <td><input id="T2" name="txt2" type="text" /></td>

            </tr>
            <tr>
                <td id="cell3"><label id="l3">Middle Name</label></td>
                <td><input id="T3" name="txt3" type="text" /></td>

            </tr>
        </table>
        <table>
                <tr>
                <td id="cell4"><label id="l4">Audience Time</label></td>
                <td><input id="hour" name="txt4" type="text" /></td>
                <td><label id="l5">hh</label></td><td><input id="min" name="txt5" type="text" /></td>
                <td><label id="l6">min</label></td>
            </tr>
        </table>
            <label id="l10">@Html.ViewData["data"]</label>
         <input id="Button1" type="submit" value="CREATE TICKET"/>
    </form>

        @section scripts
{
    <script type="text/javascript">

        $(function () {
            $.ajaxSetup({ cache: false });
            $(".compItem").click(function (e) {

                e.preventDefault();
                $.get(this.href, function (data) {
                    $('#dialogContent').html(data);
                    $('#modDialog').modal('show');
                });
            });
        })
    </script>
}

</body>
